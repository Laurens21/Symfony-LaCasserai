{% extends 'base.html.twig' %}

{% block title %}Paymentmethod{% endblock %}

{% block body %}
    <h1>Paymentmethod</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ paymentmethod.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ paymentmethod.name }}</td>
            </tr>
        </tbody>
    </table>
    <div style="display: flex">
    <a style="margin-right: 10px" class="btn btn-primary" href="{{ path('paymentmethod_index') }}">back to list</a>

    <a style="margin-right: 10px" class="btn btn-secondary" href="{{ path('paymentmethod_edit', {'id': paymentmethod.id}) }}">edit</a>

    {{ include('paymentmethod/_delete_form.html.twig') }}
    </div>
{% endblock %}
